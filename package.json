{
  "name": "dgst",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev --host",
    "build": "vite build",
    "preview": "vite preview --host",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "test": "npm run test:integration && npm run test:unit",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "postinstall": "patch-package"
  },
  "devDependencies": {
    "@playwright/test": "^1.30.4",
    "@sveltejs/adapter-node": "^1.3.1",
    "@sveltejs/kit": "^1.20.4",
    "eslint": "^8.51.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-svelte": "^2.30.0",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "4.2.19",
    "svelte-check": "^3.4.3",
    "svelte-confetti": "^1.3.2",
    "typescript": "^5.0.0",
    "vite": "4.5.5",
    "vitest": "^0.32.2"
  },
  "type": "module",
  "dependencies": {
    "@auth/core": "^0.19.1",
    "@auth/mongodb-adapter": "^2.0.0",
    "@auth/sveltekit": "^0.3.7",
    "@ckeditor/ckeditor5-build-decoupled-document": "^39.0.2",
    "@sveltejs/adapter-node": "^1.3.1",
    "@sveltestrap/sveltestrap": "^6.2.7",
    "ckeditor5-svelte": "github:techlab23/ckeditor5-svelte",
    "date-fns": "^2.30.0",
    "link-preview-js": "^3.0.5",
    "mime": "^3.0.0",
    "mongodb": "^6.10.0",
    "mongoose": "^8.8.2",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "sanitize-html": "^2.11.0",
    "svelte-loading-overlay": "^0.2.1",
    "webp-converter": "^2.3.3",
    "webp-converter-browser": "^1.0.4"
  }
}
